openapi: 3.0.0
info:
  title: Sample API
  version: 1.0.0
  # servers:
  # - url: 
paths:
  /{proxy+}:
    x-yc-apigateway-any-method:
      x-yc-apigateway-integration:
        type: serverless_containers
        container_id: SERVERLESS_CONTAINER_ID
        service_account_id: SERVICE_ACCOUNT_ID
      parameters:
      - explode: false
        in: path
        name: proxy
        required: false
        schema:
          default: '-'
          type: string
        style: simple
